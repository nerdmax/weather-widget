{
  "name": "@weather-widget/components",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build-css": "tcm --pattern 'src/**/!(global).scss' --camelCase",
    "clean-css": "shx rm -f 'src/**/*.css.d.ts' && yarn build-css",
    "postinstall": "yarn build-css",
    "kill": "lsof -ti:9001 | xargs kill",
    "start": "concurrently -k \"yarn build-css --watch\" \"yarn storybook\"",
    "storybook": "start-storybook --port 9001 --config-dir .storybook"
  },
  "dependencies": {
    "@storybook/addon-options": "^4.0.0-alpha.8",
    "@storybook/react": "^4.0.0-alpha.8",
    "@types/react": "^16.1.0",
    "@types/react-dom": "^16.0.4",
    "@types/storybook__react": "^3.0.9",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.4",
    "babel-plugin-syntax-jsx": "^6.18.0",
    "babel-plugin-syntax-typescript": "^7.0.0-beta.3",
    "classnames": "^2.2.6",
    "css-loader": "^0.28.11",
    "dotenv-webpack": "^1.5.7",
    "html-webpack-plugin": "^3.2.0",
    "identity-obj-proxy": "^3.0.0",
    "node-sass": "^4.9.3",
    "normalize.css": "^8.0.1",
    "react": "^16.3.0",
    "react-dom": "^16.3.0",
    "react-notification-system": "^0.2.17",
    "react-testing-library": "^5.0.0",
    "sass-loader": "^7.1.0",
    "shx": "^0.3.0",
    "style-loader": "^0.21.0",
    "ts-loader": "^4.4.2",
    "tsconfig-paths-webpack-plugin": "^3.2.0",
    "tslint": "^5.10.0",
    "tslint-config-prettier": "^1.13.0",
    "typed-css-modules": "^0.3.7",
    "uglifyjs-webpack-plugin": "^1.2.6",
    "webpack": "^4.16.5",
    "webpack-cli": "^3.2.0",
    "webpack-dev-server": "^3.1.1"
  }
}
